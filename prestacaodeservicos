    pragma solidity 0.5.12;

    contract prestacaoDeServicos
{
    string public prestador;
    string public empresa;
    uint256 public valor;
    uint256 public horas;

    constructor (
        string memory nomePrestador,
        string memory nomeEmpresa,
        uint256 valorHoraTrabalho,
        uint256 horasTrabalhadas) public
    {
        prestador = nomePrestador;
        empresa = nomeEmpresa;
        valor = valorHoraTrabalho;
        horas = horasTrabalhadas;
    }
    function calculoRemuneracao (uint256 valor, uint256 horas) public view returns (uint256 remuneracaoFinal)
    {
        remuneracaoFinal = valor*horas;
        return remuneracaoFinal;
    }
    
   /*view RemuneracaoFinal

    require  Horastrabalhadas = 220;

    if Horastrabalhadas>220 = "Horas inv√°lidas";

    for
    */
}
